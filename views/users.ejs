<%- include('includes/head.ejs') %>

<h1>Users</h1>

<% if (users.length > 0) { %>
    <ul>
        <%# Loop through each user and display as list item %>
        <% users.forEach((user) => { %>
            <li><%= user.name %></li>
        <% }) %>
    </ul>
<% } else { %>
    <h1>No Users Found!!</h1>
<% } %>

<%- include('includes/end.ejs') %>